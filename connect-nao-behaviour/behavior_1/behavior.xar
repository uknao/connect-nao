                                                <script language="4">
                                                    <content>
                                                        <![CDATA[
class MyClass(GeneratedClass):

    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.connman = ALProxy("ALConnectionManager")
        self.tts = ALProxy("ALTextToSpeech")
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self, p):
        self.logger.info("Processing connections: %s", str(p))

        if self.connman is None:
            self.logger.warning("No ALConnectionManager proxy")
            self.onConnectionFailed()
            return


        if self.tts is None:
            self.logger.warning("No ALTestToSpeech proxy")
            self.onConnectionFailed()
            return


        if self.memory is None:
            self.logger.warning("No ALMemory proxy")
            self.onConnectionFailed()
            return

        if len(p) != 2:
            self.logger.warning("Invalid connection parameters")
            self.onConnectionFailed()
            return

        self.ssid, self.pwd = p

        # Do a quick scan to make sure our list of services is up to date
        #self.connman.scan()

        services = [dict(s) for s in self.connman.services()]

        #Get the service id of the first service with a matching name
        matching_services = [s["ServiceId"] for s in services if s["Name"] == self.ssid]

        if len(matching_services) == 0:
            self.logger.info("Service Id not found")
            self.onConnectionFailed()
            return

        serviceId = matching_services[0]

        # Subscribe to the two callbacks, one for when creds are needed and one
        # to report whether the connection was successful.
        self.memory.subscribeToEvent("NetworkServiceInputRequired",
            self.getName(),
            "onInputRequired")

        self.memory.subscribeToEvent("NetworkConnectStatus",
            self.getName(),
            "onConnectionStatusChange")

        self.logger.info("Requesting connection to %s", serviceId)
        self.connman.connect(serviceId)
        self.logger.info("Connection to %s requested", self.ssid)

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onConnected() #activate the output of the box


    def onInputRequired(self, event_name, input_request, subscriber_id):
        self.memory.unsubscribeToEvent("NetworkServiceInputRequired", self.getName())
        self.memory.unsubscribeToEvent("NetworkConnectStatus", self.getName())

        try:
            req = dict(input_request)
            self.logger.info("Input requested: %s", str(req))

            resp = [["ServiceId", req["ServiceId"]], ["Passphrase", self.pwd]]

            self.logger.info("Sending input: %s", str(resp))
            self.connman.setServiceInput(resp)
        except ALError as e:
            self.logger.info(str(e))

    def onConnectionStatusChange(self, event_name, status, subscriber_id):

        self.memory.unsubscribeToEvent("NetworkServiceInputRequired", self.getName())
        self.memory.unsubscribeToEvent("NetworkConnectStatus", self.getName())

        if status[1]:
            self.logger.info("Connection successful")
            self.onConnected()
        else:
            if status[2].lower().find("alreadyconnected") != -1:
                self.logger.info("Already connected to that network")
                self.onConnected()
            else:
                self.logger.warning("Failed to connect, reason given: %s", status[2])
                self.onConnectionFailed()]]>
</content>
                                                </script>
